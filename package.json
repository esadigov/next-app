    "@zeit/next-typescript": "^1.1.1",
    "axios": "^1.1.3",
    "eslint-config-next": "13.0.2",
    "next": "13.0.2",
    "node-sass": "^8.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "remove": "^0.1.5",
    "sass": "^1.56.1",
    "typescript": "4.8.4"
  },
  "devDependencies": {
    "@types/jest": "^29.2.2",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
